{
  "name": "hrmodule",
  "description": "Masters Module",
  "main": ".eslintrc.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "build-staging": "NODE_ENV=production webpack --config config/webpack/staging.js",
